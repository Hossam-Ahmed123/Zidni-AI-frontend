{
  "name": "edtech-spa",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prelint": "node ./scripts/prepare-vue-tsc.mjs",
    "lint": "npm run prelint && ESLINT_USE_FLAT_CONFIG=false eslint --ext .ts,.vue src && vue-tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "cypress run --e2e",
    "test:qa": "vitest run --config vitest.config.ts --dir tests/qa --reporter verbose",
    "format": "prettier --check \"src/**/*.{ts,vue}\""
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "axios": "^1.6.8",
    "google-libphonenumber": "^3.2.37",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.9",
    "sockjs-client": "^1.6.1",
    "vue": "^3.4.21",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.3.2",
    "vuetify": "^3.5.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.5.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^1.8.27"
  }
}
